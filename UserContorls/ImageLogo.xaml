<UserControl x:Class="WalkyDog.UserContorls.ImageLogo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WalkyDog.UserContorls"
             mc:Ignorable="d" 
             Name="imageLogo">

    <Border CornerRadius="50" BorderThickness="2" BorderBrush="White" VerticalAlignment="Center">
        <Ellipse Width="23" Height="23">
            <Ellipse.Fill>
                <ImageBrush ImageSource="{Binding ElementName=imageLogo,Path=Image}" />
            </Ellipse.Fill>
        </Ellipse>
    </Border>

</UserControl>